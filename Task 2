(y → x) ∨ ¬ ((x → z) ∧ (z → x)) ∧ ¬w

print('y x z w')
for y in range(2):
    for x in range(2):
        for z in range(2):
            for w in range(2):
                if ((y <= x) or (not((x <= z) and (z <= x))) and (not w)) == 'тут пишем False,если в таблице 0. True,если в таблице 1':
                    print(y,x,z,w)
